{
  "name": "gortex-cli",
  "version": "2.0.5",
  "description": "Interactive CLI with tabbed interface and AI integration: complete Git workflow with intelligent conventional commit generation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gortex": "dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:debug": "GORTEX_DEBUG=true tsx src/index.ts",
    "dev:full": "GORTEX_ENABLE_CHAIN_OF_THOUGHT=true GORTEX_ENABLE_VERIFICATION=true tsx src/index.ts",
    "dev:full:debug": "GORTEX_DEBUG=true GORTEX_ENABLE_CHAIN_OF_THOUGHT=true GORTEX_ENABLE_VERIFICATION=true tsx src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "start:debug": "GORTEX_DEBUG=true node dist/index.js",
    "start:full": "GORTEX_ENABLE_VERIFICATION=true node dist/index.js",
    "start:full:debug": "GORTEX_DEBUG=true GORTEX_ENABLE_VERIFICATION=true node dist/index.js",
    "start:ultimate": "GORTEX_ENABLE_CHAIN_OF_THOUGHT=true GORTEX_ENABLE_VERIFICATION=true node dist/index.js",
    "start:ultimate:debug": "GORTEX_DEBUG=true GORTEX_ENABLE_CHAIN_OF_THOUGHT=true GORTEX_ENABLE_VERIFICATION=true node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage"
  },
  "keywords": [
    "git",
    "commit",
    "conventional-commits",
    "cli",
    "changelog",
    "git-hooks",
    "commit-message",
    "semantic-versioning",
    "developer-tools"
  ],
  "author": "Arthur Jean",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArthurDEV44/gortex-cli.git"
  },
  "bugs": {
    "url": "https://github.com/ArthurDEV44/gortex-cli/issues"
  },
  "homepage": "https://github.com/ArthurDEV44/gortex-cli#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    ".gortexrc.example"
  ],
  "dependencies": {
    "chalk": "^5.6.2",
    "chalk-animation": "^2.0.3",
    "commander": "^14.0.2",
    "cosmiconfig": "^9.0.0",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "ink": "^6.5.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-link": "^5.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.2.0",
    "simple-git": "^3.30.0",
    "tree-sitter": "^0.25.0",
    "tree-sitter-javascript": "^0.25.0",
    "tree-sitter-typescript": "^0.23.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "ink-testing-library": "^4.0.0",
    "strip-ansi": "^7.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.23.0"
}
